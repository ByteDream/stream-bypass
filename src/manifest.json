{
  "manifest_version": 2,
  "name": "Stream Bypass",
  "author": "ByteDream",
  "description": "",
  "version": "1.0.0",
  "homepage_url": "https://github.com/ByteDream/stream-bypass",
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "*://streamtape.com/*",
        "*://vidoza.net/*",
        "*://vivo.sx/*",
        "*://vupload.com/*"
      ],
      "js": [
        "match.js",
        "index.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_icon": "icons/stream-bypass.png",
    "default_title": "Stream Bypass",
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "128": "icons/stream-bypass.png"
  }
}